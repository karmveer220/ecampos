	<queryString><![CDATA[select rownum as contador,m.n_mst_apepaterno || '     ' || m.n_mst_apematerno || '     ' || m.n_mst_nombre as nombrecompleto,
       m2.codigopersonal,m.n_mst_dni,u.n_uno_descripcion,
       to_char(to_date(m2.codigodia,'yymmdd'),'DAY DD MONTH YYYY (dd/mm/yyyy)') as codigodia,
       to_char(to_date(m2.horainicio,'hh24miss'),'hh24:mi:ss') as horainicio,
       to_char(to_date(m2.horasalida,'hh24miss'),'hh24:mi:ss') as horasalida,
       to_char(to_date(m2.horastardanza,'hh24miss'),'hh24:mi:ss') as horastardanza,
       to_char(to_date(m2.horasxcompensar,'hh24miss'),'hh24:mi:ss') as horasxcompensar,
       to_char(to_date(m2.horasabandono,'hh24miss'),'hh24:mi:ss') as horasabandono,
       (select c.n_car_descripcion from simin_cargo c where c.c_car_codigo = m.c_car_codigo ) as n_car_descripcion,
	(select t.n_tur_descripcion from srhl_turno t where t.c_tur_codigo = m.c_tur_codigo ) as horario
from srhl_marcacion_v2 m2,simin_maestro m,simin_unidadorganica u
where m.c_mst_codigo = m2.codigopersonal
and u.c_uno_codigo = m.c_uno_codigo_of_destaque
and to_char(to_date(m2.codigodia,'yymmdd'),'dd/mm/yyyy') between '01/12/2009' and '30/12/2009'
and u.n_uno_descripcion like 'OFITEL%']]></queryString>

